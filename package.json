{
  "name": "infotrack-seo-app",
  "version": "1.0.0",
  "description": "InfoTrack Development Project",
  "main": "./app/client/index.js",
  "scripts": {
    "build:server": "babel ./app/server -d build/server",
    "build:watch:server": "babel ./app/server -d build/server --watch",
    "build:client": "webpack --config ./webpack.prod.config.js/",
    "build:watch:client": "webpack --env dev --progress --profile --colors --config ./webpack.config.js/ --watch",
    "build": "npm run build:server && npm run build:client",
    "start": "npm run build && NODE_ENV=production node ./build/server/index.js",
    "start:dev": "parallelshell 'npm run build:watch:server' 'npm run build:watch:client && NODE_ENV=dev' 'nodemon ./build/server/index.js'",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "infotrack",
    "seo"
  ],
  "author": "Bobby Joseph (bobbyj79@gmail.com)",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.18.3",
    "carbon-components": "^8.21.0",
    "carbon-components-react": "^5.49.3",
    "carbon-icons": "^6.3.2",
    "cheerio": "^1.0.0-rc.2",
    "express": "^4.16.3",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "request": "^2.86.0",
    "request-promise": "^4.2.2"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.11",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "nodemon": "^1.17.4",
    "parallelshell": "^3.0.2",
    "style-loader": "^0.21.0",
    "url-loader": "^1.0.1",
    "webpack": "^4.8.3",
    "webpack-cli": "^2.1.3"
  }
}
